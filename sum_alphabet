input_text = input ()
text_lenght = Ien (input_text)
    print (text_lenght)
    print (type (text_lenght))

vowes_sum = 0 
    for char_pos in range (text_lenght)
        current_char = input_text [char_pos]
        print (current_char)
        if current_char == "a":
            vowes_sum +=1
        elif current_char == "e"
        vowes_sum += 2
        elif current_char == "i"
            vowes_sum += 3
        elif current_char == "o"
            vowes_sum += 4
        elif current_char == "u"
            vowes_sum += 5

print (vowes_sum)