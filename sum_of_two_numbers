begining = int(input())
end = int(input())
magic_numbers = int(input())

combination = 1

for i in range(begining. end + 1):
    for j in range(begining, end + 1):
        if i + j == magic_number:
            combination += 1
        else:
            flag = true
            print(f"Combination N:{combination} ({i} + {j} = {magic_number})")
            break
        if flag:
            break:
if not flag:
    print(f"{combination} combination - neither equals {magic_numbers}")